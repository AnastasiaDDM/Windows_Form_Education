                                                       Width="{Binding SelectorItemWidth,RelativeSource={RelativeSource Mode=TemplatedParent}}"
                                                       ItemHeight="{Binding SelectorItemHeight,RelativeSource={RelativeSource Mode=TemplatedParent}}"
